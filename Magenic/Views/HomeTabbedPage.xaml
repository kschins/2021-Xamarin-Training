<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:Magenic"
            x:Class="Magenic.HomeTabbedPage">
  <NavigationPage Title="Dashboard">
      <x:Arguments>
          <local:DashboardPage />
      </x:Arguments>
  </NavigationPage>
    <NavigationPage Title="My Profile">
      <x:Arguments>
          <local:ProfilePage />
      </x:Arguments>
  </NavigationPage>
</TabbedPage>
